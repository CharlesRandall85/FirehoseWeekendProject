<div class="custom-div-header">
	<div class="span6 offset4 text-center">
		<h1>Add Your New Picture!</h1>
			<div class="boxy">
				<%= simple_form_for @pic do |f| %>
					<%= f.input :hour %>
					<%= f.input :image, :as => :file %>
					<%= f.input :emotion %>
					<%= f.input :learned %>
					<%= submit_tag  "Submit", :class => 'btn-info btn btn-large' %>
				<% end %>
			</div>
	</div>
</div>